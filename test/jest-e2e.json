{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "yes/haun/(.*)": "<rootDir>/../libs/haun/src/$1",
    "yes/haun": "<rootDir>/../libs/haun/src"
  }
}